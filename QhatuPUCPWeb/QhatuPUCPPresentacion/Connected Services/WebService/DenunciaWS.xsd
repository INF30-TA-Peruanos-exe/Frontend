          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="publicacionesFacultades">
        <xs:complexType>
          <xs:sequence>
            <xs:element minOccurs="0" maxOccurs="unbounded" name="facultad" type="tns:facultad" />
